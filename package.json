{
  "name": "@odqnger/lumosui",
  "version": "1.0.1-alpha.0",
  "description": "A React library that provides a list of UI components to help create your app.",
  "author": "oDqnger",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "components",
    "styles/output.css",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "tailwindcss": "^3.4.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "webpack --config webpack.config.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
